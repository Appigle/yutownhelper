<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Activity.PopwinUIActivity">

    <Button
        android:id="@+id/btn_empty_hongbao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="红包被领完了弹窗"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btn_opent_hongbao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拆红包弹窗"
        android:layout_marginLeft="31dp"
        android:layout_alignTop="@+id/btn_empty_hongbao"
        android:layout_toRightOf="@+id/btn_empty_hongbao"
        android:layout_toEndOf="@+id/btn_empty_hongbao"
        android:layout_marginStart="31dp" />

    <Button
        android:id="@+id/button13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拆红包弹窗"
        android:layout_below="@+id/btn_empty_hongbao"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="红包被领完了弹窗"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button13"
        android:layout_alignParentLeft="true" />

    <Button
        android:id="@+id/button15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拆红包弹窗"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/button13"
        android:layout_alignEnd="@+id/button13" />

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="红包被领完了弹窗"
        android:layout_below="@+id/button15"
        android:layout_alignRight="@+id/button13" />

    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拆红包弹窗"
        android:layout_alignTop="@+id/button15"
        android:layout_alignRight="@+id/btn_opent_hongbao"
        android:layout_alignEnd="@+id/btn_opent_hongbao" />

    <Button
        android:id="@+id/button18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="红包被领完了弹窗"
        android:layout_below="@+id/button17"
        android:layout_alignRight="@+id/btn_opent_hongbao" />

    <Button
        android:id="@+id/button19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拆红包弹窗"
        android:layout_alignTop="@+id/button14"
        android:layout_alignRight="@+id/button20"
        android:layout_alignEnd="@+id/button20" />

    <Button
        android:id="@+id/button20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="红包被领完了弹窗"
        android:layout_above="@+id/button19"
        android:layout_alignRight="@+id/btn_opent_hongbao" />
</RelativeLayout>
